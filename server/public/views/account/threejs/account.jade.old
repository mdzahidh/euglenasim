doctype html
html
  head
    block head
      title #{title}
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='stylesheet', href='/layouts/core.min.css?#{cacheBreaker}')
    link(rel='stylesheet', href='/layouts/default.css')
    link(rel='stylesheet', href='/layouts/myCanvas.css')
    link(rel='stylesheet', href='/layouts/myJoystick.css')
    link(rel='stylesheet', href='/vendor/jquery-ui/themes/start/jquery-ui.css')
    block neck
  body
    div.navbar.navbar-default.navbar-fixed-top
      div.container
        div.navbar-header
          a.navbar-brand(href='/account/')
            img.navbar-logo(src='/media/logo-symbol-64x64.png')
            span.navbar-brand-label #{projectName}
          button.navbar-toggle.collapsed(data-toggle='collapse', data-target='.my-navbar-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
        div.navbar-collapse.my-navbar-collapse.collapse
          ul.nav.navbar-nav
            //li: a(href='/account/liveview/') Normal Replay
            //li: a(href='/account/procview/') Computerized Replay 
            li: a(href='/account/modeling/') Model
            li: a(href='/account/joymodel/') JoyModel
            //li: a(href='/account/tracking/') Tracking
          ul.nav.navbar-nav.pull-right
            li: a(href='/logout/')
              i.fa.fa-user
              |  Sign Out

    div.page
      div.container
        block body

    div.footer
      div.container
        div.inner
          span.copyright.pull-right
            |&#169; #{copyrightYear} #{copyrightName}
          ul.links
            li: a(href='/') Home
            li: a(href='/logout/') Sign Out
          div.clearfix

    div.ajax-spinner
      img(src='/media/ajax-pulse.gif')

    script(src='/layouts/core.min.js?#{cacheBreaker}')

    block feet
